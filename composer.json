{
    "name": "epistol/cdg",
    "description": "Cuisine De Geek",
    "keywords": [
        "geek",
        "cooking",
        "websites"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^2.2",
        "algolia/scout-extended": "^1.6",
        "askedio/laravel5-profanity-filter": "^1.10",
        "barryvdh/laravel-cors": "^0.11.0",
        "cybercog/laravel-ban": "^3.5",
        "dimsav/laravel-translatable": "^9.4",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/psr7": "^1.5",
        "htmlmin/htmlmin": "^5.7",
        "intervention/image": "^2.4",
        "itsgoingd/clockwork": "^2.0",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/passport": "5.0.*",
        "laravel/scout": "^7.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "~1.0",
        "lorisleiva/laravel-deployer": "^0.2.9",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.37",
        "mews/purifier": "^2.1",
        "paragonie/random_compat": "2.*",
        "php-http/curl-client": "^1.7",
        "pragmarx/firewall": "^2.2",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.11.0",
        "sightengine/client-php": "^1.3",
        "socialiteproviders/instagram": "^3.0",
        "spatie/laravel-feed": "^2.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.36",
        "spatie/laravel-responsecache": "^4.2",
        "spatie/laravel-sitemap": "^5.2",
        "spatie/schema-org": "^1.4",
        "torann/geoip": "^1.0",
        "vinkla/hashids": "^5.1",
        "waavi/sanitizer": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "dev-master",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "squizlabs/php_codesniffer": "^3.4",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^8.0",
        "laravel/telescope": "^1.0",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "phpcs": "phpcs --standard=PSR2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
