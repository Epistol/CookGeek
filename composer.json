{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "askedio/laravel5-profanity-filter": "dev-master",
        "barryvdh/laravel-cors": "^0.11.0",
        "bestmomo/nice-artisan": "^1.1",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "itsgoingd/clockwork": "^2.0",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "5.0.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "mews/purifier": "^2.1",
        "sentry/sentry-laravel": "^0.8.0",
        "spatie/laravel-feed": "^2.0",
        "spatie/laravel-permission": "^2.23",
        "spatie/laravel-responsecache": "^4.2",
        "spatie/schema-org": "^1.4",
        "tcg/voyager": "^1.1",
        "torann/geoip": "^1.0",
        "voyager-redirects": "^1.0",
        "yab/laravel-scout-mysql-driver": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "dev-master",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/telescope": "^0.1.6",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
