{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "askedio/laravel5-profanity-filter": "dev-master",
    "barryvdh/laravel-cors": "^0.11.0",
    "cybercog/laravel-ban": "^3.5",
    "dimsav/laravel-translatable": "^9.3",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "~4.0",
    "guzzlehttp/psr7": "^1.5",
    "intervention/image": "^2.4",
    "itsgoingd/clockwork": "^2.0",
    "laravel/cashier": "~7.0",
    "laravel/framework": "5.8.*",
    "laravel/horizon": "^2.0",
    "laravel/passport": "5.0.*",
    "laravel/scout": "^6.0",
    "laravel/socialite": "^4.0",
    "laravel/tinker": "~1.0",
    "mews/purifier": "^2.1",
    "paragonie/random_compat": "2.*",
    "php-http/curl-client": "^1.7",
    "pragmarx/firewall": "^2.2",
    "predis/predis": "^1.1",
    "sentry/sentry-laravel": "^0.11.0",
    "spatie/laravel-feed": "^2.0",
    "spatie/laravel-permission": "^2.23",
    "spatie/laravel-responsecache": "^4.2",
    "spatie/laravel-sitemap": "^5.2",
    "spatie/schema-org": "^1.4",
    "tcg/voyager": "^1.1",
    "torann/geoip": "^1.0",
    "vinkla/hashids": "^5.1",
    "voyager-redirects": "^1.0",
    "yab/laravel-scout-mysql-driver": "^2.0",
    "moontoast/math": "*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "dev-master",
    "barryvdh/laravel-ide-helper": "^2.*",
    "filp/whoops": "~2.*",
    "fzaninotto/faker": "~1.4",
    "laravel/dusk": "^5.*",
    "laravel/homestead": "^8.*",
    "laravel/telescope": "^1.*",
    "mockery/mockery": "0.9.*",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "~6.*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "phpcs": "phpcs --standard=PSR2"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "repositories": {
    "hooks": {
      "type": "composer",
      "url": "https://larapack.io"
    }
  }
}
